{
  "name": "@blossom-labs/connect-bl-tao-voting",
  "version": "0.2.1",
  "license": "LGPL-3.0-or-later",
  "description": "Access and interact with Aragon Tao Voting app.",
  "author": "Blossom Labs",
  "homepage": "https://github.com/BlossomLabs/connect-tao-voting/",
  "bugs": "https://github.com/BlossomLabs/connect-tao-voting/issues",
  "repository": "https://github.com/BlossomLabs/connect-tao-voting/",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "ethereum",
    "javascript",
    "web3",
    "dapp",
    "aragon"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc --build",
    "build:cjs": "tsc --build ./tsconfig.cjs.json",
    "dev:esm": "yarn build:esm --watch",
    "dev:cjs": "yarn build:cjs --watch",
    "clean": "rm -rf ./dist ./tsconfig.tsbuildinfo",
    "lint": "eslint --ext .ts ./src"
  },
  "dependencies": {
    "@1hive/connect-core": "^0.9.13",
    "@1hive/connect-thegraph": "^0.9.13",
    "@1hive/connect-types": "^0.9.13",
    "graphql-tag": "^2.10.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.34",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "typescript": "^4.8.4"
  }
}
