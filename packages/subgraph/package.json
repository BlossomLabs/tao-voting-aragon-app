{
  "name": "@blossom-labs/subgraph-tao-voting",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "codegen": "rm -rf generated; graph codegen",
    "build": "rm -rf build; graph build",
    "manifest-mainnet": "scripts/build-manifest.sh mainnet",
    "manifest-mainnet-staging": "env STAGING=true scripts/build-manifest.sh mainnet",
    "manifest-gnosis": "scripts/build-manifest.sh gnosis",
    "manifest-optimism": "scripts/build-manifest.sh optimism",
    "deploy-mainnet": "scripts/deploy.sh blossomlabs bl-tao-voting mainnet",
    "deploy-mainnet-staging": "env STAGING=true scripts/deploy.sh blossomlabs bl-tao-voting mainnet",
    "deploy-gnosis": "scripts/deploy.sh blossomlabs bl-tao-voting gnosis",
    "deploy-optimism": "scripts/deploy.sh blossomlabs bl-tao-voting optimism",
    "create-local": "graph create tao-voting --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy tao-voting --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.35.0",
    "@graphprotocol/graph-ts": "^0.28.1",
    "mustache": "^4.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
